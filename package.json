  "publisher": "changxiongwang9",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createCTEXbookProject",
        "title": "Create CTEXbook project"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gocpicnic/latex-project-generator.git"
  },
  "author": "gocpicnic",
  "license": "GPL3.0",
  "bugs": {
    "url": "https://github.com/gocpicnic/latex-project-generator/issues"
  },
  "homepage": "https://github.com/gocpicnic/latex-project-generator/blob/main/README.md",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
